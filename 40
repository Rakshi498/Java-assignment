// function that checks if a given number is prime.

function primenumber(num) {
    if (num < 2) {
     return false;
    }
    for (let i = 2; i * i <= num; i++) {
    if (num % i === 0) {
    return false;
    }
    return true;
    }
}
   
console.log(primenumber(7)); //true
console.log(primenumber(4)); //false

