//Use the following arithmetic operators on a and b

let a=10;
let b=5;

// Addition

let sum=a+b;
console.log(sum);

// Substraction

let sub=a-b;
console.log(sub);

// Multiplication

let mul=a*b;
console.log(mul);

// Division

let div=a/b;
console.log(div);

// Modulus

let mod=a%b;
console.log(mod);

output:
       15
        5
       50
        2
        0




